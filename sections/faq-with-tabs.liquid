<div
  class="faq-tabs-{{ section.id }} faq-tabs {% unless section.settings.full_width_section %}page-width {% endunless %}section-{{ section.id }}-padding color-{{ section.settings.color_scheme }}"
  id="faq_tabs_container"
>
  <div class="">
    <div class="tab-links">
      {% for block in section.blocks %}
        {% if forloop.length > 1 %}
          <div class="tab-link" data-tab="tab-{{ block.id }}">
            {% if block.settings.tab_icon != blank %}
              <div class="tab-icon">
                {{
                  block.settings.tab_icon
                  | image_url: width: block.settings.tab_icon.width, height: block.settings.tab_icon.height
                  | image_tag
                }}
              </div>
            {% else %}
              <div class="tab-icon default">
                {{ default_image }}
              </div>
            {% endif %}
            {{ block.settings.tab_title | default: 'Top Question' }}
          </div>
        {% endif %}
      {% endfor %}
      <div class="tab-line"></div>
    </div>

    <div class="tab-contents">
      {% for block in section.blocks %}
        <div id="tab-{{ block.id }}" class="tab-content" style="display:none;">
          {% if block.settings.faq_title_1 != blank %}
            <details class="details">
              <summary>
                {{ block.settings.faq_title_1 }}
                <span class="open_icon">+</span>
              </summary>
              <div class="details-content">
                {{ block.settings.faq_content_1 }}
              </div>
            </details>
          {% endif %}

          {% if block.settings.faq_title_2 != blank %}
            <details class="details">
              <summary>
                {{ block.settings.faq_title_2 -}}
                <span class="open_icon">+</span>
              </summary>
              <div class="details-content">
                {{ block.settings.faq_content_2 }}
              </div>
            </details>
          {% endif %}

          {% if block.settings.faq_title_3 != blank %}
