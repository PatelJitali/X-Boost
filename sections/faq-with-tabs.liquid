              <summary>
                {{ block.settings.faq_title_4 -}}
                <span class="open_icon">+</span>
              </summary>
              <div class="details-content">
                {{ block.settings.faq_content_4 }}
              </div>
            </details>
          {% endif %}

          {% if block.settings.faq_title_5 != blank %}
